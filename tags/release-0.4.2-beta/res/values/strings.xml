    <string name="app_name">GeoKrety Logger</string>
    
    <!-- Main Activity -->
    <string name="welcome">GeoKrety Logger is a program to logging of moving of GeoKrets. GeoKrety (<a href="http://en.m.wikipedia.org/wiki/Geokrety">en.m.wikipedia.org/wiki/Geokrety</a>) is a similar game to TravelBug(TM) or GeoLutins, aiming at tracking objects you leave in geocache containers (<a href="http://en.m.wikipedia.org/wiki/Geocaching">en.m.wikipedia.org/wiki/Geocaching</a>).</string>
    <string name="accounts_button">Accounts</string>
    <string name="button_inventory">Inventory</string>
    <string name="button_ocs">OpenCaching last logs</string>
    <string name="button_log">Log GeoKret</string>
    <string name="button_about">About</string>
    <string name="version">GeoKrety Logger v</string>
    <string name="copyright">Copyright &#169; by Michał Niedźwiecki 2013, 2014</string>
    <string name="about">WWW: <a href="http://geokretylog.sourceforge.net/">geokretylog.sourceforge.net</a></string>
    <string name="emergency">If this program not working, please visit and log on the <a href="http://geokrety.org/ruchy.php">geokrety.org/ruchy.php</a> web page. Afterwards please contact with me on the web site: <a href="http://geokrety.org/majluj.php?to=12444">http://geokrety.org/majluj.php?to=12444</a>.</string>
    <string name="help_in_translation">Suggest a better translation:\n<a href="http://geokretylog.sourceforge.net/translate.php">geokretylog.sourceforge.net/translate.php</a></string>
